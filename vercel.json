{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*\\.(png|jpg|jpeg|svg|gif|ico))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
